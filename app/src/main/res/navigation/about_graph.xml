<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/about_graph"
    app:startDestination="@id/aboutFragment">


    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.externalpods.hcprodemo.presentation.AboutFragment"
        android:label="About"
        tools:layout="@layout/fragment_about" >
        <action
            android:id="@+id/action_aboutFragment_to_aboutDetailsFragment"
            app:destination="@id/aboutDetailsFragment" />
    </fragment>

    <fragment
        android:id="@+id/aboutDetailsFragment"
        android:name="com.externalpods.hcprodemo.presentation.AboutDetailsFragment"
        android:label="About details"
        tools:layout="@layout/fragment_about_details" />

</navigation>